{
  "manifest_version": 3,
  "name": "Table Exporter",
  "version": "1.0",
  "description": "Export table data to CSV",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon48.png",
      "48": "icon48.png",
      "128": "icon48.png"
    }
  },
  "icons": {
    "16": "icon48.png",
    "48": "icon48.png",
    "128": "icon48.png"
  }
}
