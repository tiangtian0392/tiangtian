	商品网址 = "view-source:"&商品网址
	TracePrint(商品网址)
	PrintToScreen.DrawText(objWindow,"商品网址"&商品网址,18,[255,0,0])
	Clipboard.SetText(商品网址)
	#icon("@res:jp9omu83-hrmn-ahdv-6i18-2gga6vrvkedu.png")
