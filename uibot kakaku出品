	在库出力表名 = Replace(在库出力,".xlsx","",False)
	iRet = Dialog.MsgBox("在库出力文件-"&在库出力表名&"是否正确，はい-确认，いいえ-重新选择","UiBot",1,2,0)
	If iRet=6
		Break
		
	Else
		
	End If
Loop
