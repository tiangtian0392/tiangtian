	商家  = [["ヤマダウェブコム",1865,111],["PREMOA",3713,101],["コジマ",3832,101],["エディオン",3779,101],["コジマネット",1925,11],["タンタン",130,111],["WiNK",5,111],["EDIONネットショップ",1747,111],["ひかりTVショッピング",2162,111],["ソフマップ",1824,111],["阪通",915,110],["ノジマオンライン",1639,100],["特価.com",3830,100],["ECJOY!",1396,100],["ECカレント",74,11],["GIGA",1356,11],["特価.com",20,10]]
	全商家 = []
