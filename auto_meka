Rem 型号整理
Function XingHaoZL()
	strXH = Clipboard.GetText()
	strXH_list = Regex.FindAll(strXH,"[\\S]")
	tesuzifu = ["[","]","(",")"," ","　"]
	strXH_text = ""
	For Each value In strXH_list
		For Each value_item In tesuzifu
			If value_item=value
				value = "-"
				Break
				
			Else
				
			End If
			
		Next
		
		strXH_text = strXH_text&value
	Next
	
	strXH_PD = EndsWith(strXH_text,"-")
	If strXH_PD
		
		strXH_text = RTrim(strXH_text,"-")
	Else
		
	End If
	strXH_CD = Len(strXH_text)
	If strXH_CD>20
		strXH_text = Replace(strXH_text,"-","",False)
		
	Else
		
	End If
	strXH_CD1 = Len(strXH_text)
	If strXH_CD1>20
		TracePrint(">20")
		
		iRet = Dialog.MsgBox("字符串长度：" & strXH_CD & "\n" & strXH_text,"UiBot","0","1",0)
	Else
		
	End If
	Clipboard.SetText(strXH_text)
	strXH_text = strXH_text&":"&strXH_CD1
	TracePrint(strXH_CD1,strXH_text)
End Function
