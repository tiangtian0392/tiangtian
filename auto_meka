				Else
					详情 = Regex.FindStr(详情,value_详情,0)
					
				End If
				
				TracePrint(value_详情,详情)
				JS = 1+JS
			Next
		Else
			
		End If
		
	Next
	
	Rem 去掉商品详情内图片，动画，商家文字
	rec_GX = ["商品情報","\\<div class=\"goodscomment2_ \">[\\s\\S]+?\\</div>","[^>]+について[≫|＞]*","商品説明","\\<font[\\s\\S]+?\\</font>","\\<img[\\s\\S]+?>","\\<a[\\s\\S]+?\\</a>","\\<iframe[\\s\\S]+?\\</iframe>","[^>]+について[≫|＞]*"]
