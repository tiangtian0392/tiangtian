		arrRet = Regex.FindAll(详情,value)
		TracePrint(arrRet)
		For Each value_GX In arrRet
			详情 = Replace(详情,value_GX,"",False)
			
		Next
		
	Next
	TracePrint(JAN,型号,详情)
End Function
Rem 下载amazon图片
Function GetIMAGE()
	meka_Amazon = Regex.FindStr(kakaku_htmlcode,"ShopCD="&meka_dict["Amazon.co.jp"][0]&"[\\s\\S]+?(?=\")",0)
	Amazon_URL = "https://c.kakaku.com/forwarder/forward.aspx?"&meka_Amazon
	Amazon_URL = Replace(Amazon_URL,"&amp;","&",False)
	hWeb_img = WebBrowser.Create("chrome",Amazon_URL,30000,{"bContinueOnError":False,"iDelayAfter":300,"iDelayBefore":200,"sBrowserPath":"","sStartArgs":""})
	IMG_code = WebBrowser.GetHTML(hWeb_img,{"bContinueOnError":False,"iDelayAfter":300,"iDelayBefore":200})
	IMG_code = Regex.FindStr(IMG_code,"id=\"landingImage[\\s\\S]+?</div>",0)
	list_image = Regex.FindAll(IMG_code,"http[\\s\\S]+?jpg")
	size = 0
	amaon_URL = ""
	For Each value In list_image
		value_img = Regex.FindStr(value,"(?<=AC_S\\S)\\d+",0)
		value_img = CInt(value_img)
		If value_img>size
			size = value_img
			amaon_URL = value
			
		Else
			
		End If
		
	Next
	
	TracePrint(amaon_URL)
	DownImage(amaon_URL)
End Function
Rem 下载图片
Function DownImage(DowURL)
	TracePrint("开始下载网页，输出型号，URL",型号,DowURL)
	hWeb_img = WebBrowser.BindBrowser("chrome",10000,{"bContinueOnError":False,"iDelayAfter":300,"iDelayBefore":200})
	Try 3
		WebBrowser.Download(hWeb_img,DowURL,"C:\\Users\\user\\Pictures\\"&型号&".jpg",True,100000,{"bContinueOnError":False,"iDelayAfter":300,"iDelayBefore":200})
	Catch e
		TracePrint("Amazon图片下载失败")
	Else
		iPID = App.Start("C:\\Users\\user\\Pictures\\"&型号&".jpg", "0", "1")
	End Try
	
End Function


